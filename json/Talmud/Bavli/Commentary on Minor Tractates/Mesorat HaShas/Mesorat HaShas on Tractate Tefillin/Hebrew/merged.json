            [
                "ריש מסכת מזוזה וש\"נ עד שכתבן עברית:"
            ],
            [],
            [
                "ברכות כ' וש״מ:",
                "סוכה מ\"ב א׳ ערכין ב׳ ב׳:",
                "ירושלמי ריש פ\"י דעירובין וש\"מ:",
                "עירובין צ״ו א' וש״מ:"
            ],
            [
                "ברכות י״ז ב׳ וע\"ש בתוס׳ וש\"פ וכגירסת הירושלמי שם:"
            ],
            [],
            [],
            [
                "עי׳ גיטין מ\"ה ב׳ וש״מ:"
            ],
            [
                "מנחות מ״ב ב׳:",
                "עי׳ עירובין צ\"ז א׳ ובירושלמי פ״י שם ובתוספתא פ״ג דע׳׳ז הביא תוס׳ במנחות מ\"ג א' ע\"ש:"
            ],
            [
                "מנחות ל\"ד ב׳:",
                "א״ר יוסי מודה כצ״ל ע\"ש:"
            ],
            [
                "שם ל״ז ב':",
                "שם א':"
            ],
            [
                "עי׳ ברכות ט׳ ב׳ סוכה מ\"ו א׳:",
                "מנחות ל״ו א׳:",
                "סוכה כ\"ו ב׳:",
                "מנחות ל״ו א׳:"
            ],
            [
                "שם ל״ח א':",
                "שם ל״ו א׳:",
                "שבת ס״א א׳:",
                "עי׳ סנהדרין צ״ב א' כל שאינו משייר פת כו׳:"
            ],
            [
                "מגילה כ״ד ב׳:"
            ],
            [
                " עירובין דף צ״ה א׳:",
                "שם דף צ״ו ב׳:"
            ],
            [
                "שם דף צ״ה ב׳:",
                "עי׳ ביצה דף ט\"ו א׳:",
                "עי׳ ברכות דף כ״ה א׳:"
            ],
            [
                "ע״ש דף כ״ג ב׳:",
                "ע״ש דף כ״ד א׳:",
                "עי׳ ב\"מ דף ק״ה ב׳:"
            ],
            [
                "שבת דף י׳ א׳:"
            ],
            [
                "ברכות דף כ״ג א׳:"
            ],
            [
                "סוכה דף כ״ח א׳:"
            ],
            [
                "מדרש ש״ט:"
            ],
            [
                "מנחות דף מ״ג ב׳:",
                "ספרי ס״פ ואתחנן: "
            ]
        ]
