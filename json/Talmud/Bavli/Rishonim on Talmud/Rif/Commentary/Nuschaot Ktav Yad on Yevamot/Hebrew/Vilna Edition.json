            "דמיתנא דמן אבוהי:",
            "אפי' קרוב:",
            "לדיינים:",
            "גם בצרתה:"
        ],
        [
            "מאי וכן:"
        ],
        [
            "באדם אחד:",
            "דחלה דאנס:",
            "יבם קטן אנוס הוא ולא הוי כארוסה שהגיע זמן:",
            "סימני שער כו':"
        ],
        [],
        [],
        [
            "כנפי השכינ':"
